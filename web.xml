<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Patient_Insert</servlet-name>
        <servlet-class>Patient_insert.Patient_Insert</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Patient_Delete</servlet-name>
        <servlet-class>Patient_Delete</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Patient_Update</servlet-name>
        <servlet-class>Patient_Update</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Patient_Retrieve</servlet-name>
        <servlet-class>Patient_Retrieve</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Patient_Insert</servlet-name>
        <url-pattern>/Patient_Insert</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Patient_Delete</servlet-name>
        <url-pattern>/Patient_Delete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Patient_Update</servlet-name>
        <url-pattern>/Patient_Update</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Patient_Retrieve</servlet-name>
        <url-pattern>/Patient_Retrieve</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
